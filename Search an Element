#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 
    int n,n1;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d\n",&arr[i]);
    }
    scanf("%d",&n1);
    int c=0;
    for(int i=0;i<n;i++){
        if(arr[i]==n1){
            c++;
        }
    }
    if(c>0){        
    printf("%d is presented in an array.\n",n1);
        }
     else{
            printf("%d is not presented in an array.\n",n1);
        }
    return 0;
}
