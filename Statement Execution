#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int a;
    float b;
    char c;
    char d[50];
    char e[50];
    scanf("%d %f  %c  ",&a,&b,&c);
    scanf("%s ",d);
    fgets(e,50,stdin);
    printf("Integer is %d\n",a);
    printf("Float is %0.2f\n",b);
    printf("Character is %c\n",c);
    printf("Strings are %s and %s",d,e);
    return 0;
}
