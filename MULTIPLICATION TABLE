#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n,n1;
    scanf("%d",&n);
    for(int i=1;i<=n;i++){
        n1=n*i;
        printf("%d * %d = %d\n",n,i,n1);
    }
    
    return 0;
}
