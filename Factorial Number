#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n;
    scanf("%d",&n);
    int mul=1;
    for(int i=1;i<=n;i++){
        mul*=i;
    }
    if(n<0){
        printf("Invalid Input");
    }
    else{
    printf("The Factorial of %d is %d",n,mul);
    }
    return 0;
}
