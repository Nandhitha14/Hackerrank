#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
        int s,l,w,b1,side,b2;
    float r;
    scanf("%d\t%d\t%d\t%f\t%d\t%d\t%d",&s,&l,&w,&r,&b1,&side,&b2);
    printf("Perimeter of Square:%d\n",4*s);
    printf("Perimeter of Rectangle:%d\n",2*(l+w));
    printf("Perimeter of Circle:%.2f\n",floor((2*3.14159*r)*100)/100);
    printf("Perimeter of Triangle:%d\n",b1+side+b2);
    return 0;
    return 0;
}
