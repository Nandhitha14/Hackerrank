#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n;
    scanf("%d",&n);
    int n1=n%10;
    int n2=n/10;
    printf("Bird said:%d\n",n);
    printf("Alice must go in path-%d\n",n1+n2);
    return 0;
}
