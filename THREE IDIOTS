#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    float xa, ya;
    int x1, y1, x2, y2;
    scanf("%d\n%d\n%d\n%d", &x1, &y1, &x2, &y2);
    xa = (x1+x2)/2.0;
    ya = (y1+y2)/2.0;
    printf("Binoy's house is located at (%.1f,%.1f)", xa, ya);
    return 0;
}
