#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n;
    scanf("%d",&n);
    int temp=n;
    int sum=0;
    for(int i=1;i<n;i++){
        if(temp%i==0){
        sum+=i;
      }
    }
    if(sum>n){
        printf("Abundant Number");
    }
    else{
        printf("Not Abundant Number");
    }
    return 0;
}
