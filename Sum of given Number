#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n;
    int sum=0;
    do{
        scanf("%d\n",&n);
        if(n==-1){
            break;
        }
        else{
           sum+=n; 
        }
    }
    while(sum);
        printf("%d\n",sum);
    
    return 0;
}
