#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int s,l,b,r,ba,h;
    scanf("%d\t%d\t%d\t%d\t%d\t%d",&s,&l,&b,&r,&ba,&h);
    printf("Area of Square=%d\n",s*s);
    printf("Area of Rectangle=%d\n",l*b);
    printf("Area of Circle=%.2f\n",floor((3.14159*r*r)*100)/100);
    printf("Area of Triangle=%.0f\n",0.5*ba*h);
    return 0;
}
