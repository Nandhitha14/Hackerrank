#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
  
    char str1[100], str2[100];

    // Input two strings
    scanf("%s", str1);
    scanf("%s", str2);

    // Concatenate str2 to str1
    strcat(str1, str2);

    // Output the result
    printf("%s\n", str1);

    return 0;
}
