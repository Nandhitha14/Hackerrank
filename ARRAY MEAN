#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {  
    int n;
    float mean=0;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d\t",&arr[i]);
    }
    for(int i=0;i<n;i++){
        mean+=arr[i];
    }
    printf("Array Mean Value is %.2f",mean/n);
    return 0;
}
