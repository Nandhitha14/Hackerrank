#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char a, b;
    scanf("%c", &a);
    b = toupper(a);
    if(b=='A'||b=='E'||b=='I'||b=='O'||b=='U'){
        printf("The Character %c is Vowel", a);
    }
    else{
        printf("The Character %c is Consonant", a);
    }
    return 0;
}
